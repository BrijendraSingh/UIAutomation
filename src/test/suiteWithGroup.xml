<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="UI Automation Test Suite" verbose="1" >
    <test name="Group Automation Test" >
        <groups>
            <run>
                <include name="testGroup"></include>
            </run>
        </groups>
        
        <classes>
            <class name="com.thoughtworks.SpreeTests"></class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.thoughtworks.reporting.ExtentReporterNG"></listener>
    </listeners>
</suite>
